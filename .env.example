# Server Configuration
PORT=3000

# Cache TTL in milliseconds (default: 5 minutes)
CACHE_TTL_MS=300000

# Maximum pages to fetch in multi-page mode
MAX_PAGES=10

# Auth token for API downloads (get from browser after login)
AUTH_TOKEN=your_jwt_token_here

# Referrer URL for hotlink protection (default: https://not.ultranx.ru)
REFERRER=https://not.ultranx.ru

# Enable request logging (true/false)
LOG_REQUESTS=true

# Batch size for parallel game detail fetches
BATCH_SIZE=5

# Multi-page loading (pages to fetch at once)
MULTI_PAGE_COUNT=3
# Fetch timeout in milliseconds (default: 15 seconds)
FETCH_TIMEOUT_MS=15000